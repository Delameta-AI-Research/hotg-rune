FROM runicos/base

CAPABILITY<U8[1, 96, 96]> image IMAGE  #--sampling_frequency_ms 500

# PROC_BLOCK<U8[1, 96, 96],f32[1, 96, 96]> normalize hotg-ai/rune#proc_blocks/normalize

