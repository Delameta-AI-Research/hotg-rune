FROM runicos/base

CAPABILITY<i16[24000]> audio SOUND --hz 16000 --samples 150 --sample-size-ms 1500  # set up libraries and interfaces

CAPABILITY<I16[24000]> audio SOUND --hz 16000 --samples 150 --sample-size-ms 1500
PROC_BLOCK<I16[24000],I16[49, 40]> fft hotg-ai/rune#proc_blocks/fft

PROC_BLOCK runicos/proc-block/fft fft

MODEL<F32[150],F32> ./model.tflite model --input [150,1] --output 1

MODEL<I16[49, 40],F32[4,1]> model ./model.tflite
RUN audio fft model

OUT serial
