FROM runicos/base

CAPABILITY<U8[1, 96, 96]> image IMAGE --pixel-format @PixelFormat::GrayScale

MODEL<U8[96, 96],U8[3]> person_detection ./model.tflite

PROC_BLOCK<U8[3],U32[1]> most_confident_index hotg-ai/rune#proc_blocks/most-confident-indices --count=1
PROC_BLOCK<U32[1],UTF8[1]> label hotg-ai/rune#proc_blocks/label --labels=unknown,person_prob,not_person_prob

OUT SERIAL

RUN image person_detection most_confident label serial
